<form action="/autoshop" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <p>Enter a new shop name:</p>
  <input type='text' name='autoshop[name]'/><input type='submit'/>
  <p>Is it open?<input type="checkbox" id="autoshop[open]" name="open" value=true /></p>
</form>
