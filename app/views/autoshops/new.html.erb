<a href="/parents">Back to Main</a>
<a href="/autoshops">Go to Auto Shops</a>
<a href="/vehicles">Go to Vehicles</a>

<%= form_with url: '/autoshops', method: :post, local:true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>

  <%= form.label :open %>
  <%= form.check_box :open %>

  <%= form.label :vehicles_in_shop %>
  <%= form.text_field :vehicles_in_shop %>

  <%= form.submit "Create Auto Shop" %>
<% end %>

<body style="background-color:powderblue;">
</body>
