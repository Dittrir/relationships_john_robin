<h3><%= link_to "Plumbers", "/plumbers" %> <%= link_to "PlumberShops", "/plumbershops"%> <%= link_to "Back to Main", "/parents" %></h3>
<h3><%= link_to "Alphabetize Plumbers", "/plumbershops/#{@plumbershop.id}/plumbers?sort=asc" %></h3>
<% @plumbers.each do |plumber| %>
<h3><li><%= link_to "#{plumber.name}", "/plumbers/#{plumber.id}" %></li></h3>
<p>License: <%= plumber.license %></p>
<p>On Call: <%= plumber.on_call %></p>
<p>Years of Experience: <%= plumber.years_experience %></p>
<p>Created At: <%= plumber.created_at %></p>
<p>Updated At: <%= plumber.updated_at %></p>
<p><%= button_to "Delete #{plumber.name}", "/plumbers/#{plumber.id}", method: :delete %></p>
<% end %>

<h3><%= link_to "New Plumber", "/plumbershops/#{@plumbershop.id}/plumbers/new" %></h3>
<%= form_with url: "/plumbershops/#{@plumbershop.id}/plumbers", method: :get, local: true do |form| %>
<%= form.label :search, "Desired Years Of Experience" %>
<%= form.text_field :search %>
<%= form.submit "Submit" %>
<% end %>

<body style="background-color:powderblue;">
  </body>
